{
  "expo": {
    "name": "Augmenta",
    "slug": "Augmenta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/MAYA.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "package": "com.augmenta.app",
      "supportsTablet": true
    },
    "android": {
      "jsEngine": "jsc",
      "package": "com.augmenta.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f177d117-11d6-4233-87fd-1b303a7850d8"
      }
    }
  }
}
